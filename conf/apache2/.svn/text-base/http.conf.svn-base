<VirtualHost *:80>
  ServerName www.papa.partio.fi
  ServerAlias *

  # Enable mod_rewrite
  RewriteEngine On
  RewriteLog /var/log/apache2/rewrite-http.log
  RewriteLogLevel 1
  ProxyPreserveHost On

  RewriteCond %{HTTP_HOST} !=www.papa.partio.fi
  RewriteRule ^(.*)$ http://www.papa.partio.fi$1 [redirect,qsappend,last]

  RewriteRule ^(/+.+\.php)$ http://vanha.papa.partio.fi$1 [redirect=permanent,qsappend,last]

  RewriteCond %{HTTP_HOST} ^pakki.papa.partio.fi$
  RewriteRule ^/*$ http://www.papa.partio.fi/pakki [redirect,qsappend,last]

  # Zope
  RewriteRule ^/zope(.*)$ http://localhost:8080/VirtualHostBase/http/www.papa.partio.fi:80/VirtualHostRoot/_vh_zope$1 [proxy,last]

  # Partio.nyt
  RewriteRule ^/partio.nyt($|/.*)$ http://localhost:8081/VirtualHostBase/http/www.papa.partio.fi:80/pt-syys-sm-2011/VirtualHostRoot/_vh_partio.nyt$1 [proxy,last]

  # Main Plone
  RewriteRule ^(.*)$ http://localhost:8080/VirtualHostBase/http/www.papa.partio.fi:80/Plone/VirtualHostRoot$1 [proxy]
</VirtualHost>
